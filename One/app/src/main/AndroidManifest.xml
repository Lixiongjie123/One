<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell_user.one">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="org.litepal.LitePalApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Fragment.samll_fragment.CreatActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".IntentActivity.SearchActivity"/>
        <activity android:name=".IntentActivity.OtherSettingsActivity"/>
        <activity android:name=".IntentActivity.UserbackActivity"/>
        <activity android:name=".IntentActivity.MusicIntentActivity"/>
        <activity android:name=".IntentActivity.MovieIntentActivity"/>
        <activity android:name=".MainActivity">
        </activity>
    </application>

</manifest>